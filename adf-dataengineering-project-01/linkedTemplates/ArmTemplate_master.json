			"type": "string",
			"defaultValue": "AKIATOVBFOWFT6PHP24K"
		},
		"AmazonS32_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIATOVBFOWF2X3RAJVV"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saretailchain.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1003-044259-9fohpx0a"
		},
		"KV_DataEngin_Project01_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvdataengproject01.vault.azure.net/"
		},
		"LSADLSGen2SQL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2dataengproject.dfs.core.windows.net/"
		},
		"LSRestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummyjson.com/products"
