					},
					"AmazonS32_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS32_properties_typeProperties_accessKeyId')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"KV_DataEngin_Project01_properties_typeProperties_baseUrl": {
						"value": "[parameters('KV_DataEngin_Project01_properties_typeProperties_baseUrl')]"
					},
					"LSADLSGen2SQL_properties_typeProperties_url": {
						"value": "[parameters('LSADLSGen2SQL_properties_typeProperties_url')]"
					},
					"LSRestAPI_properties_typeProperties_url": {
						"value": "[parameters('LSRestAPI_properties_typeProperties_url')]"
					},
					"lsblobstorage_sasUri": {
						"value": "[parameters('lsblobstorage_sasUri')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-dataengineering-project-01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_OnPrem_SQLServer_DEProj01_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_OnPrem_SQLServer_DEProj01_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-dataengineering-project-01_ArmTemplate_0')]"
			]
		}
	]
}